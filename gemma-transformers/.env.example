# Gemma3n API Configuration

# Model settings
MODEL_NAME=google/gemma-3n-e4b
GEMMA3N_PORT=8000
MAX_LENGTH=2048
TEMPERATURE=0.7

# Docker image settings
BASE_IMAGE=dustynv/pytorch:2.1-r36.2.0
GEMMA3N_TAG=gemma3n-api:latest

# GPU settings
NVIDIA_VISIBLE_DEVICES=all

# Hugging Face token (required for gated models)
# HF_TOKEN=your_huggingface_token_here

# Model cache directory (to persist downloaded models)
MODEL_CACHE_DIR=./models